library(tggl)
"fista_tggl" %in% getNamespaceExports("tggl")
fista_tggl_cpp_full <- function(X, Y, tree, lambda, L,
max_iter = 500, tol = 1e-6, verbose = FALSE) {
fista_tggl_full_cpp(
X             = X,
Y             = Y,
lambda        = lambda,
L             = L,
indices_list  = lapply(tree, function(nd) nd$index),
weight_vec    = sapply(tree,       function(nd) nd$weight),
node_order    = get_postorder_indices(tree),
max_iter      = as.integer(max_iter),
tol           = as.numeric(tol),
verbose       = verbose
)
}
